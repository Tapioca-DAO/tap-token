{
    "name": "tap-token",
    "license": "MIT",
    "scripts": {
        "compile": "yarn clean && NODE_ENV=compile hardhat compile --quiet && yarn solhint",
        "solhint": "solhint contracts/**/*.sol --fix",
        "clean": "rm -rf ./cache && rm -rf ./artifacts && rm -rf ./.openzeppelin",
        "typechain": "npx hardhat typechain",
        "test": "npx hardhat test",
        "coverage": "yarn clean && NODE_ENV=compile hardhat coverage",
        "contracts_size": "yarn compile && NODE_ENV=hardhat hardhat size-contracts",
        "verify_contract": "hardhat verify --network",
        "@comment deploy": "Deploy scripts",
        "deploy:TapOFT": "hardhat deploy --tags TapOFT --network",
        "deploy:Governance": "hardhat deploy --tags Governance --network",
        "deploy:LiquidityGauge": "hardhat deploy --tags LiquidityGauge --network",
        "generate-sdk": "npx hardhat exportSDK"
    },
    "devDependencies": {
        "@boringcrypto/boring-solidity": "boringcrypto/BoringSolidity#e0ff4a7771edfa09a90694c4e0f891938cf62a43",
        "@ethersproject/abi": "^5.7.0",
        "@ethersproject/providers": "^5.7.0",
        "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
        "@nomicfoundation/hardhat-network-helpers": "^1.0.4",
        "@nomicfoundation/hardhat-toolbox": "^1.0.2",
        "@nomiclabs/hardhat-ethers": "^2.1.1",
        "@nomiclabs/hardhat-etherscan": "^3.1.0",
        "@nomiclabs/hardhat-vyper": "^3.0.0",
        "@openzeppelin/contracts": "^4.5.0",
        "@primitivefi/hardhat-dodoc": "^0.2.3",
        "@typechain/ethers-v5": "^10.1.0",
        "@typechain/hardhat": "^6.1.2",
        "@types/chai": "^4.2.22",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.11",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "chai": "^4.3.6",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "ethers": "^5.7.0",
        "hardhat": "^2.6.5",
        "hardhat-contract-sizer": "^2.5.1",
        "hardhat-deploy": "^0.9.6",
        "hardhat-gas-reporter": "^1.0.8",
        "http-server": "^14.0.0",
        "mocha": "^9.1.3",
        "prb-math": "^2.4.1",
        "prettier": "^2.5.1",
        "prettier-plugin-solidity": "^1.0.0-beta.19",
        "solc": "^0.8.4",
        "solhint": "^3.3.6",
        "solidity-coverage": "^0.7.21",
        "ts-node": "^10.2.1",
        "typechain": "^8.1.0",
        "typescript": "^4.4.3",
        "write-json-file": "^4.3.0"
    },
    "dependencies": {
        "@prb/math": "^2.5.0"
    }
}
